    {
      "name": "post-request script",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "const response = pm.response.json();\r",
              "console.log(\"Created Object ID: \" + response.id);\r",
              "\r",
              "pm.environment.set(\"objectId\", response.id);\r",
              "\r",
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Check if Name is correct\", function () {\r",
              "    pm.expect(response.name).to.eql(\"Apple MacBook Pro 16\");\r",
              "});\r",
              "\r",
              "pm.test(\"Verify ID is exactly 123\", function () {\r",
              "    pm.expect(response.id).to.eql(\"123\");\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
